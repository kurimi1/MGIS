type Response {
	Total int    `json:"total"`
	Kcdjs []Kcdj `json:"kcdjs"` // 矿产地基本情况
}

type KqdzRsp {
	Total int    `json:"total"`
	Kqdzs []Kqdz `json:"kqdzs"` // 矿区地质情况
}

type KcclRsp {
	Total int    `json:"total"`
	Kccls []Kccl `json:"kccls"` // 矿产储量
}

type KcjjRsp {
	Total int    `json:"total"`
	Kcjjs []Kcjj `json:"kcjjs"` // 矿床技术经济评价
}

type KcjsRsp {
	Total int   `json:"total"`
	Kcjs  []Kcj `json:"kcjs"` // 开采技术条件
}

type kckcRsp {
	Total int    `json:"total"`
	Kckcs []Kckc `json:"kckcs"` // 矿产勘查工作概况
}

type kcmcRsp {
	Total int    `json:"total"`
	Kcmcs []Kcmc `json:"kcmcs"` // 主要可采煤层特征
}

type kcqlRsp {
	Total int    `json:"total"`
	Kcqls []Kcql `json:"kcqls"` // 勘查区资源量
}

type kttzRsp {
	Total int    `json:"total"`
	Kttzs []Kttz `json:"kttzs"` // 矿体特征
}

type mctzRsp {
	Total int    `json:"total"`
	Mctzs []Mctz `json:"mctzs"` // 煤矿体特征
}

type xksyRsp {
	Total int    `json:"total"`
	Xksys []Xksy `json:"xksys"` // 选矿试验
}

@server(
	prefix: /api
)
service MGIS-api {
	@doc(
		summary: "获取矿产地基本情况"
	)
	@handler List
	get /K(Request) returns (Response)
	
	@doc(
		summary: "获取矿区地质情况"
	)
	@handler Kqdz
	get /Kqdz(Request) returns (KqdzRsp)
	
	@doc(
		summary: "获取矿产储量"
	)
	@handler Kccl
	get /Kccl(Request) returns (KcclRsp)
	
	@doc(
		summary: "获取矿床技术经济评价"
	)
	@handler Kcjj
	get /Kcjj(Request) returns (KcjjRsp)
	
	@doc(
		summary: "获取开采技术条件"
	)
	@handler Kcjs
	get /Kcjs(Request) returns (KcjsRsp)
	
	@doc(
		summary: "获取矿产勘查工作概况"
	)
	@handler Kckc
	get /Kckc(Request) returns (kckcRsp)
	
	@doc(
		summary: "获取主要可采煤层特征"
	)
	@handler Kcmc
	get /Kcmc(Request) returns (kcmcRsp)
	
	@doc(
		summary: "获取勘查区资源量"
	)
	@handler Kcql
	get /Kcql(Request) returns (kcqlRsp)
	
	@doc(
		summary: "获取矿体特征"
	)
	@handler Kttz
	get /Kttz(Request) returns (kttzRsp)
	
	@doc(
		summary: "获取煤矿体特征"
	)
	@handler Mctz
	get /Mctz(Request) returns (mctzRsp)
	
	@doc(
		summary: "获取选矿试验"
	)
	@handler Xksy
	get /Xksy(Request) returns (xksyRsp)
}